<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.projeto.meda.meda">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/comida_tambem_e"
        android:label="MEDA"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".SplashScreen"
            android:label="MEDA">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".TelaLogin"
            android:label="MEDA">
            <intent-filter>
                <action android:name="android.projeto.meda.meda.TelaLogin" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".TelaRecuperarSenha" />
        <activity android:name=".TelaCadastrarUsuario" />
        <activity android:name=".TelaInicialUsuarioComum" />
        <activity android:name=".TelaInformacaoUsuario" />
        <activity android:name=".TelaEditaInfoUsuario" />
        <activity android:name=".TelaSobre"></activity>
    </application>

</manifest>